{
  "name": "myresful",
  "version": "1.0.1-beta.0",
  "description": "A package for handling API requests with built-in version check",
  "main": "dist/index.js",
  "browser": "dist/myresful.umd.js",
  "module": "dist/myresful.esm.js",
  "exports": {
    "import": "./dist/myresful.esm.js",
    "require": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "babel src -d dist && npm run rollup",
    "start": "node dist/index.js",
    "rollup": "npx rollup -c",
    "test": "echo \"Write tests!\"",
    "preversion": "npm run build",
    "version": "npm run build && git add .",
    "postversion": "node scripts/version-helper.cjs",
    "release:beta": "npm version prerelease --preid=beta && npm publish --tag beta",
    "release": "npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orangKota026/myresful.git"
  },
  "keywords": [
    "RESTful",
    "API",
    "ECMAScript"
  ],
  "author": "Basuni Shibah <basuniproject@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "rollup": "^4.34.8",
    "rollup-plugin-terser": "^7.0.2"
  },
  "engines": {
    "node": "^20 || ^18 || ^16 || ^14"
  },
  "type": "module"
}
